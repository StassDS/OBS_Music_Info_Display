{
    "manifest_version": 3,
    "name": "Music Track Parser",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://music.youtube.com/*", "https://plaza.one/*"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": [
      "http://localhost:5000/*"
    ]
  }
  